me='ConferenceList'>\r\n            \r\n\r\n         \r\n        <div class=\"Conlist\">\r\n            <div class=\"container\">\r\n            \r\n                <div class=\"row\">\r\n               \r\n                    <div class=\"col\">\r\n                   <br></br>\r\n                   \r\n                        <div class=\"row\">\r\n                   \r\n                          {workshops.filter((p)=>p.status == \"approved\").map((workshops, index) => (\r\n\r\n                                <Card\r\n                                    topic={workshops.topic}\r\n                                    description={workshops.description}\r\n                                    // name={workshops.date}\r\n                                    email={workshops.email}\r\n                                    \r\n                                    \r\n                                    // link= {papers.researchpaper[0].fil}\r\n                                    link ={ \"http://localhost:8065/uploads/\"+workshops.worshopproposol[0].fil}\r\n                                    workshopProposal={workshops.worshopproposol[0].fil}\r\n                                   \r\n                                />\r\n                                \r\n\r\n                            ))}\r\n                              </div> \r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n             </div>\r\n             </div>\r\n             </Layout>\r\n    )\r\n\r\n}\r\n\r\nexport default WorkshopList;"},"lineCount":null}},"error":null,"hash":"ba160420db53efd8c19900801ae80d70","cacheData":{"env":{}}}